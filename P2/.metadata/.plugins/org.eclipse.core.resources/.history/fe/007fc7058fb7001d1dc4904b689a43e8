package omegaup;
import java.util.Scanner;
import java.text.DecimalFormat;
public class Main {
	public static void main(String[] args){
		DecimalFormat formateador = new DecimalFormat("###");
		Scanner in = new Scanner(System.in);
		int c = in.nextInt();
		double k = c+273.15;
		double f = (c*1.8)+32;
		double r = c*.8;
		System.out.print(formateador.format(k ));
		System.out.print(formateador.format(f ));
		System.out.print(formateador.format(r));
	}
}

//System.out.print(formateador.format(imc));