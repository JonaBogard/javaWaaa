package omegaup;
import java.util.Scanner;
public class Main {
	public static void main(String[] args){
		Scanner in = new Scanner(System.in);
		int r=in.nextInt();
		int a=in.nextInt();
		
		for(int i =0;i<r;i++) {
			for(int j=0;j<a;j++) {
				System.out.print("*");
			}
			System.out.println(" ");
		}
		}
	}

